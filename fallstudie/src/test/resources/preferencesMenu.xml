        <optionbox name="Styles of boxes" dependsOn="dataEditor_useStyledBoxes=true">
          <option id="dataEditor_style_rdfterm" name="RDFTerm" datatype="boxStyleSequence" value="ROUNDBOX | #FF7F00 | #FF0000" />
